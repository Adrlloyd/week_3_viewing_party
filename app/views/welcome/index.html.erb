<% if session[:user_id] %>
    <%= button_to "Log Out", logout_path, method: :post  %>
<section class='existing-users'>
    <p>Existing Users: </p>
    <% @users.each do |user| %>
        <li><%= user.email %></li>
    <% end %>
    <% else %>
<%= button_to "Create New User", register_path, method: :get %>
<%= button_to "Log In", login_path, method: :get %>
<% end %>
</section>